1 =  количество  int;
NaturalToLow(количество, количество);


NaturalToLow void(int n, int count)
{
    если (количество > n)
    {
        Возврат;
    }
    ещё
    {
        NaturalToLow(n, количество  + 1);
        Консоль.Запись (количество + " ");
    }
} 
